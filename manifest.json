{
  "manifest_version": 3,
  "name": "Extension Privacy Analyzer",
  "version": "1.0",
  "description": "Analyzes permissions of installed extensions and reports potential privacy risks.",
  "permissions": [
    "management",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module" 

  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}